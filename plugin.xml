<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    id="cordova-plugin-cleanup"
    version="1.0.0">
    <name>Cleanup Plugin</name>
    <description>Cordova plugin to delete specified files after prepare</description>
    <license>Apache 2.0</license>
    <keywords>cordova,cleanup,delete,files</keywords>
    
    <preference name="CLEANUP_PATHS" default="" />
    
    <hook type="after_prepare" src="hooks/after_prepare.js" />
</plugin>